inductive ℕ: Type
{
	0: ℕ,
	suc: ℕ → ℕ,
}

inductive Vec: (Type, ℕ) → Type
{
	nil: (A: Type) → Vec A ℕ.0,
	cons: (A: Type, n: ℕ, Vec A n, A) → Vec A (ℕ.suc n),
}

def prepend(A: Type, n: ℕ, v: Vec A n, a: A): Vec A (ℕ.suc n)
{
	match v to [A: Type, n: ℕ, Vec A n] Vec A (ℕ.suc n)
	{
		nil _ => Vec.cons A ℕ.0 (Vec.nil A) a,
		cons _ n' v' a' => Vec.cons A n (rec v') a',
	}
}
